{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "max-classes-per-file": false,
    "no-console": false,
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "semicolon": false
  }
}
